{
  "name": "console.img",
  "version": "2.2.0",
  "description": "ðŸŽ‰ Display a picture in the Chrome browser console",
  "repository": {
    "url": "https://github.com/u3u/console.img",
    "type": "git"
  },
  "main": "dist/console.img.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm run lint && dtslint types",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "build": "bili --filename console.img[suffix].js --moduleName console.img --format umd,umd-min --banner",
    "format": "prettier \"*/**/*.js\" --ignore-path ./.prettierignore --write",
    "prettier": "npm run format && npm run lint:fix && git status"
  },
  "author": "u3u <qwq@qwq.cat>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bili": "^3.1.2",
    "dtslint": "^0.5.4",
    "eslint-config-rem": "^4.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.0",
    "xo": "^0.23.0"
  },
  "pre-commit": [
    "prettier"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "xo": {
    "extends": "rem",
    "envs": [
      "browser"
    ],
    "rules": {
      "space-before-function-paren": 0
    }
  }
}
